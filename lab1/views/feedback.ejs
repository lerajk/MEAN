<link href='https://fonts.googleapis.com/css?family=Open+Sans:700,600' rel='stylesheet' type='text/css'>

<form method="post" action="thanks">
<div class="box">
<h1>Feedback Form</h1>

<!-- INPUTS -->
<input name="firstname" value="firstname" onFocus="field_focus(this, 'email');" onblur="field_blur(this, 'email');" class="email"/>

<input name="lastname" value="lastname" onFocus="field_focus(this, 'email');" onblur="field_blur(this, 'email');" class="email" />

<input type="email" name="email" value="<%= title %>" onFocus="field_focus(this, 'email');" onblur="field_blur(this, 'email');" class="email" />

<input name="comments" value="comments" onFocus="field_focus(this, 'email');" onblur="field_blur(this, 'email');" class="email" />

<input name="favoritesubject" value=" favorite subject" onFocus="field_focus(this, 'email');" onblur="field_blur(this, 'email');" class="email" />
<input name="favoriteactor" value=" favorite actor" onFocus="field_focus(this, 'email');" onblur="field_blur(this, 'email');" class="email" />


  
<!-- <a href="#"><div class="btn">Log In</div></a> -->
 <input type="submit" value="Submit" class="btn"/>
  
</div> <!-- End Box -->
  
</form>

